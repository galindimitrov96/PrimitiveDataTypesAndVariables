using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace DeclareVariables
{
    class Program
    {
        static void Main(string[] args)
        {
            sbyte a = -115;
            byte b = 97;
            short c = -10000;
            ushort d = 52130;
            int e = 4825932;
            Console.WriteLine("ushort = {0}, sbyte = {1}, int = {2}, byte = {3}, short = {4}", d,a,e,b,c);

        }
    }
}
