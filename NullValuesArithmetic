using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication23
{
    class Program
    {
        static void Main(string[] args)
        {
            int? a = null;
            double? b = null;
            Console.WriteLine(a);
            Console.WriteLine(b);          
            a = a + 19;
            b = b + 4.5;
            Console.WriteLine(a);
            Console.WriteLine(b);
            a = 20;
            b = 12.5;
            Console.WriteLine(a);
            Console.WriteLine(b);
        }
    }
}
